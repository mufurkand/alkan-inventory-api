generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Part {
  id           Int     @id @default(autoincrement())
  materialType String
  partNumber   String  @unique
  location     String?
  price        String?
  quantity     String?
  channel      String?
  caseType     String?
  voltage      String?
  current      String?
  value        String?
  unit         String?
  power        String?
  description  String?
  imagePath    String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
